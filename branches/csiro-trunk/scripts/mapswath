    echo	"41) Lord Howe Rise 1	162.5/162.75/-34.3/-33.95"
    echo 	"42) Lord Howe Rise 2	158.85/159.15/-29.3/-29.15"
    echo	"43) Lord Howe Rise 3	159.05/159.4/-32/-31.6"
    echo	"44) Lord Howe Rise 4 	160.3/162.7/-32.8/-32"
