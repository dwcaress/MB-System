#!/bin/sh
#
#  mapswath
#
#  Script to allow interactive selection of data set to map.
#

NULLDIR=/tmp/empty
if [ ! -d $NULLDIR ] ; then
	mkdir $NULLDIR
fi

SWATHSRC=$NULLDIR
export SWATHSRC

while [ -z "$SWATHDIR" ]; do

	echo
	echo Please select swath data set:
	echo 
	echo "0.  All swath data"
	echo "1.  EM1002 raw data"
        echo "2.  Bluefin EM3000 data"
	echo "3.  EM300 raw data"
	echo "4.  SS0404 survey data (EM300 raw)"
        echo "5.  NORFANZ (EM300 raw Tangaroa)"
	echo "7.  SS0407 survey data (EM300 raw)"
	echo "9.  SS0409 survey data (EM300 raw)"
	echo
	echo "10.  All xyz (cleaned) data"
        echo "11.  EM1002 xyz (cleaned) data"
        echo "13.  EM300 xyz (cleaned) data"
	echo "14.  SS0404 processed data (Neptune xyz)"
        echo "17.  SS0407 processed data (Neptune xyz)"
	echo
	echo "20.  All GA Data"
	echo "21.  GA 0.1 degree (public) data"
#	echo "22.  GA swath XYZ data"
	echo "23.  GA west Tas XYZ data"
	echo "24.  GA east Tas XYZ data"
	echo "25.  GA AUSTREA1"
	echo "26.  GA cleaned swath data (GSF)"
	echo "27.  Tas seamounts"
	echo "28.  Tas seamounts gridded"
	echo "29.  All xyz data (including restricted GA)"
	echo
	echo "30.  EM300 trials (Perth Canyon)"
	echo
	echo "99.  All data"
	echo
	echo "100. Current directory (./datalist.mb-1)"
	echo "101. Working data"
	echo

	read DATASET

	case "$DATASET" in
	0)
		SWATHDIR=/home/swath/mbproc/allswath
		DATANAME=All_restricted
	;;
	1)
		SWATHDIR=/home/swath/mbproc/EM1002/
		DATANAME=em1002
	;;
        2)
                SWATHDIR=/home/swath/mbproc/bluefin_swath_trials/
		DATANAME=bluefin
        ;;
	3)
		SWATHDIR=/home/swath/mbproc/em300/
		DATANAME=em300
	;;
	4)
		SWATHDIR=/home/swath/mbproc/em300/SE_Ecosystem/
		DATANAME=ss0404
	;;
        5)
                SWATHDIR=/home/swath/mbproc/gx30_NORFANZ/
		DATANAME=NORFANZ
        ;;
	7)
		SWATHDIR=/home/swath/mbproc/em300/SS200407/
		DATANAME=ss0407
	;;
	9)
		SWATHDIR=/home/swath/mbproc/em300/SS200409/
		DATANAME=ss0409
	;;
	10)
		SWATHDIR=/home/swath/mbdata/xyz/
		DATANAME=xyz
	;;
	11)
		SWATHDIR=/home/swath/mbdata/xyz/em1002/
		DATANAME=em1002_xyz
	;;
	13)
		SWATHDIR=/home/swath/mbdata/xyz/em300/
		DATANAME=em300_xyz
	;;
	14)
		SWATHDIR=/home/swath/ss0404/xyz
		DATANAME=ss0404_xyz
	;;
	17)
		SWATHDIR=/home/swath/ss0407/xyz
		DATANAME=ss0407_xyz
	;;
	20)
		SWATHDIR=/home/swath/mbdata/xyz/ga_restricted/
		DATANAME=GA_restricted
	;;
	21)
		SWATHDIR=/home/swath/mbdata/xyz/ga_bathy/
		DATANAME=GA
	;;
	23)
		SWATHDIR=/home/swath/mbdata/xyz/ga_restricted/west_162/
		DATANAME=GA_restricted
	;;
	24)
		SWATHDIR=/home/swath/mbdata/xyz/ga_restricted/east_162/
		DATANAME=GA_restricted
	;;
	25)
		SWATHDIR=/home/swath/mbdata/xyz/ga_restricted/austrea/
		DATANAME=GA_restricted
	;;
	26)
		SWATHDIR=/home/swath/mbdata/gsf/
		DATANAME=GA_GSF_restricted
	;;
	27)
		SWATHDIR=/home/swath/mbdata/xyz/tas_seamounts/
		DATANAME=GA_restricted
	;;
        28)
                SWATHDIR=/home/swath/mbdata/xyz/tas_seamounts_grid/
                DATANAME=GA_restricted
        ;;

	29)
		SWATHDIR=/home/swath/mbdata/
		DATANAME=xyz_restricted
	;;
	30)
		SWATHDIR=/home/swath/mbproc/em300/EM300trials
		DATANAME=em300
	;;
	99)
		SWATHDIR=/home/swath/mbproc/alldata
		DATANAME=All_restricted
	;;
	100)
		SWATHDIR=.
		DATANAME=.
	;;
	101)
		SWATHDIR=/home/swath/mbproc/working
		DATANAME=working
	;;
	esac

