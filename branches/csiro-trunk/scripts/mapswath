    echo	"32) Lacepede 50m 	139.37/139.47/-36.58/-36.49"
    echo	"33) Lacepede 80m	138.3/138.37/-37.01/-36.96"
    echo 	"34) Lacepede 120m 	138.23/138.31/-37.09/-37.04"
